{
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "catppuccin.accentColor": "peach",
  "catppuccin.colorOverrides": {
    "mocha": {
      "base": "#010101",
      "crust": "#020202",
      "mantle": "#010101"
    }
  },
  "css.lint.unknownAtRules": "ignore",
  "debug.javascript.autoAttachFilter": "disabled",
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderSideBySide": false,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "always",
  "editor.bracketPairColorization.enabled": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 10,
  "editor.fontFamily": "'JetBrains Mono', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.glyphMargin": true,
  "editor.guides.bracketPairsHorizontal": false,
  "editor.guides.indentation": false,
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "relative",
  "editor.linkedEditing": true,
  "editor.minimap.maxColumn": 100,
  "editor.rulers": [100],
  "editor.stickyScroll.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[Horizon Bold]": {
      "comments": "#7a7a7a"
    },
    "[Horizon]": {
      "comments": "#7a7a7a"
    },
    "[Tokyo Night]": {
      "comments": "#7a7a7a"
    }
  },
  "editor.wordWrap": "on",
  "errorLens.onSave": true,
  "eslint.format.enable": true,
  "eslint.run": "onSave",
  "explorer.fileNesting.enabled": false,
  "explorer.fileNesting.patterns": {
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.ts": "${capture}.js",
    "*.tsx": "${capture}.module.scss",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
    "tsconfig.json": "tsconfig.*.json"
  },
  // Vim Performance
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "files.associations": {
    "*.csp": "html",
    "*.sass": "sass",
    "[Dd]ockerfile*": "dockerfile",
    "docker-compose*": "yaml"
  },
  "files.eol": "\n",
  "files.exclude": {
    "**/*.env": false,
    "**/.idea/": true,
    "**/.next/": true,
    "**/.storybook/": true,
    "**/.yarn/": true,
    "**/build/": true,
    "**/dist/": true,
    "**/node_modules/": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.ignoreRebaseWarning": true,
  "git.mergeEditor": false,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.enable": {
    "*": true
  },
  "gitlens.advanced.fileHistoryFollowsRenames": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.scopes": [],
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.menus": {
    "editor": false,
    "editorGroup": false,
    "editorTab": false,
    "explorer": false,
    "scm": false,
    "scmGroup": false,
    "scmGroupInline": false,
    "scmItem": false
  },
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.statusBar.enabled": false,
  "inline-parameters.border": "1px solid #222",
  "inline-parameters.fontSize": "0.75em",
  "inline-parameters.fontWeight": "200",
  "inline-parameters.hideSingleParameters": true,
  "inline-parameters.showVariadicNumbers": false,
  "javascript.format.enable": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "peacock.affectActivityBar": false,
  "peacock.affectTitleBar": false,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#dd0531"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#61dafb"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Svelte Orange",
      "value": "#ff3d00"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "redhat.telemetry.enabled": false,
  "search.exclude": {
    "**/*.env": false,
    "**/dist": true,
    "**/playground/*": true
  },
  "search.followSymlinks": false,
  "search.smartCase": true,
  "tailwindCSS.experimental.classRegex": [
    "tw`([^`]*)", // tw`...`
    "tw=\"([^\"]*)", // <div tw="..." />
    "tw={\"([^\"}]*)", // <div tw={"..."} />
    "tw\\.\\w+`([^`]*)", // tw.xxx`...`
    "tw\\(.*?\\)`([^`]*)" // tw(Component)`...`
  ],
  "tailwindCSS.includeLanguages": {
    "typescript": "javascript",
    "typescriptreact": "javascript"
  },
  "terminal.external.windowsExec": "C:\\ProgramData\\Microsoft\\Windows\\Start Menu\\Programs\\Git\\Git Bash.lnk",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.inheritEnv": true,
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-cmd",
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ]
    },
    "Git Bash": {
      "args": ["-l"],
      "source": "Git Bash"
    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell"
    }
  },
  "terminal.integrated.scrollback": 5000,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.format.enable": true,
  "typescript.tsserver.useSyntaxServer": "never",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.camelCaseMotion.enable": true,
  "vim.changeWordIncludesWhitespace": true,
  "vim.easymotion": true,
  "vim.handleKeys": {
    "<C-/>": false,
    "<C-c>": false,
    "<C-f>": false,
    "<C-i>": true,
    "<C-k>": false,
    "<C-n>": false,
    "<C-o>": true,
    "<C-p>": false,
    "<C-r>": true,
    "<C-v>": false,
    "<C-w>": false,
    "<C-x>": false,
    "<C-y>": false
  },
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
    {
      "before": ["shift+alt+down"],
      "commands": ["editor.action.copyLinesDownAction"]
    },
    {
      "after": ["gh"],
      "before": ["ff"]
    },
    {
      "after": ["dd"],
      "before": ["D"]
    },
    // harpoon
    {
      "before": ["<leader>", "a"],
      "commands": ["vscode-harpoon.addEditor"]
    },
    {
      "before": ["<leader>", "H"],
      "commands": ["vscode-harpoon.editEditors"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["vscode-harpoon.editorQuickPick"]
    },
    {
      "before": ["<leader>", "1"],
      "commands": ["vscode-harpoon.gotoEditor1"]
    },
    {
      "before": ["<leader>", "2"],
      "commands": ["vscode-harpoon.gotoEditor2"]
    },
    {
      "before": ["<leader>", "3"],
      "commands": ["vscode-harpoon.gotoEditor3"]
    },
    {
      "before": ["<leader>", "4"],
      "commands": ["vscode-harpoon.gotoEditor4"]
    },
    {
      "before": ["<leader>", "5"],
      "commands": ["vscode-harpoon.gotoEditor5"]
    },
    // closing buffers/tabs
    {
      "before": ["<leader>", "b", "l"],
      "commands": ["workbench.action.closeEditorsToTheLeft"]
    },
    {
      "before": ["<leader>", "b", "r"],
      "commands": ["workbench.action.closeEditorsToTheRight"]
    },
    {
      "before": ["<leader>", "b", "o"],
      "commands": ["workbench.action.closeOtherEditors"]
    },
    {
      "before": ["<leader>", "b", "a"],
      "commands": ["workbench.action.closeAllEditors"]
    },
    // naavigating buffers/tabs with H/L
    {
      "before": ["H"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["L"],
      "commands": ["workbench.action.nextEditor"]
    },
    // ]e [e - go to previous/next error/marker
    {
      "before": ["[", "e"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "e"],
      "commands": ["editor.action.marker.next"]
    },
    // rebinding vscode features
    {
      "before": ["<leader>", "g"],
      "commands": ["workbench.view.scm"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.view.search"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.files.action.focusFilesExplorer"]
    },
    // Taken from https://gist.github.com/brotherkaif/098f14950d538cb7001a7e04d8d884ee
    {
      // Code Action
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"],
      "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
    },
    {
      // Source Action
      "before": ["<leader>", "c", "A"],
      "commands": ["editor.action.quickFix"],
      "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
    },
    {
      // Rename
      "before": ["<leader>", "c", "r"],
      "commands": ["editor.action.rename"],
      "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
    }
  ],
  "vim.showMarksInGutter": true,
  "vim.textwidth": 100,
  "vim.useSystemClipboard": true,
  "vim.vimrc.enable": true,
  "vim.visualModeKeyBindings": [
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.commandCenter": false,
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
  "workbench.colorCustomizations": {
